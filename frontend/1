apiVersion: apps/v1
kind: Deployment
metadata:
  name: backend-deployment
spec:
    replicas: 1
    selector:
      matchLabels:
        app: backend
    template:
      metadata:
       labels:
        app: backend
      spec:
        containers:
        - name: backend
          image: us-central1-docker.pkg.dev/my-project-123456-410817/mydocker/backend:latest
          ports:
          - containerPort: 80
